{
  "name": "Homarr",
  "version": "latest",
  "slug": "homarr",
  "description": "A sleek, modern dashboard that puts all of your apps and services at your fingertips with extensive integrations",
  "url": "https://github.com/homarr-labs/homarr",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "image": "ghcr.io/homarr-labs/homarr:latest",
  "ports": {
    "7575/tcp": 7575
  },
  "ports_description": {
    "7575/tcp": "Web interface"
  },
  "webui": "http://[HOST]:[PORT:7575]",
  "ingress": true,
  "panel_icon": "mdi:view-dashboard",
  "panel_title": "Homarr",
  "hassio_api": true,
  "docker_api": true,
  "privileged": ["NET_ADMIN"],
  "map": ["addon_config:rw"],
  "options": {
    "PORT": 7575,
    "AUTH_SECRET_KEY": ""
  },
  "schema": {
    "PORT": "port",
    "AUTH_SECRET_KEY": "str?"
  },
  "environment": {
    "PORT": "7575",
    "AUTH_SECRET_KEY": "",
    "DATABASE_URL": "file:///data/db.sqlite",
    "NEXTAUTH_URL": "http://localhost:7575"
  }
}
