{
  "name": "Homarr",
  "version": "latest",
  "slug": "homarr",
  "description": "A sleek, modern dashboard that puts all of your apps and services at your fingertips with extensive integrations",
  "url": "https://github.com/homarr-labs/homarr",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "image": "ghcr.io/homarr-labs/homarr",
  "ports": {
    "7575/tcp": 7575
  },
  "ports_description": {
    "7575/tcp": "Web interface port"
  },
  "webui": "http://[HOST]:[PORT:7575]",
  "ingress": true,
  "ingress_port": 0,
  "panel_icon": "mdi:view-dashboard",
  "panel_title": "Homarr",
  "hassio_api": true,
  "privileged": ["NET_ADMIN"],
  "full_access": false,
  "host_network": false,
  "host_pid": false,
  "host_ipc": false,
  "host_dbus": false,
  "devices": [],
  "udev": false,
  "docker_api": true,
  "video": false,
  "audio": false,
  "gpio": false,
  "usb": false,
  "uart": false,
  "kernel_modules": false,
  "homeassistant_api": false,
  "stdin": false,
  "legacy": false,
  "environment": {
    "SECRET_ENCRYPTION_KEY": ""
  },
  "options": {
    "secret_encryption_key": "",
    "port": 7575,
    "enable_docker_integration": true
  },
  "schema": {
    "secret_encryption_key": "str?",
    "port": "port",
    "enable_docker_integration": "bool"
  },
  "map": [
    "config:rw",
    "ssl"
  ]
}