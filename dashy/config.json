{
  "name": "Dashy",
  "version": "latest",
  "slug": "dashy",
  "url": "https://dashy.to/",
  "init": false,
  "description": "Dashy is an open source, highly customizable, easy to use, privacy-respecting dashboard app",
  "webui": "http://[HOST]:[PORT:8080]",
  "stage": "stable",
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "image": "ghcr.io/lissy93/dashy",
  "ports": {
    "8080/tcp": 8080
  },
  "ports_description": {
    "8080/tcp": "Web interface"
  },
  "options": {
    "/app/user-data/": "/user-data/"
  },
  "map": ["addon_config:rw"]
}
