{
  "name": "Dashboard",
  "version": "latest",
  "slug": "dashboard",
  "description": "A simple Homelab dashboard",
  "url": "https://hub.docker.com/r/phntxx/dashboard/",
  "arch": ["aarch64", "amd64", "armv7"],
  "options": {
    "PUID": "1000",
    "PGID": "1000"
  },
  "schema": {
    "PUID": "int",
    "PGID": "int"
  },
  "image": "phntxx/dashboard",
  "ports": {
    "3460/tcp": 8080
  },
  "ports_description": {
    "3460/tcp": "web interface"
  },
  "webui": "http://[HOST]:[PORT:3460]",
  "map": [
    "addon_config:rw"
  ]
}
